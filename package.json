{
  "name": "straweb-portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "start": "netlify build && ng run straweb-portfolio:serve:ssr",
    "prestart": "ng build --configuration production && ng run straweb-portfolio:server:production",
    "poststart": "node dist/straweb-portfolio/server/main.js",
    "postbuild": "ng run straweb-portfolio:prerender",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "nety": "netlify dev",
    "serve:ssr": "ng run straweb-portfolio:serve:ssr",
    "build:ssr": "ng build --configuration production && ng run straweb-portfolio:server:production",
    "prerender": "ng run straweb-portfolio:prerender",
    "netlify:serve": "ng run straweb-portfolio:serve:ssr:production",
    "netlify:build": "ng build --configuration production && ng run straweb-portfolio:server:production",
    "start:prod": "npm run netlify:build && npm run serve:ssr:straweb-portfolio",
    "cy:open": "cypress open",
    "cy": "cypress run",
    "test:cy": "start-server-and-test start http://localhost:8888 cy:run",
    "cy:run": "cypress run --browser chromium",
    "serve:ssr:straweb-portfolio": "node dist/straweb-portfolio/server/main.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.5",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/material": "^20.2.5",
    "@angular/platform-browser": "^20.3.2",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/ssr": "^20.3.3",
    "@netlify/angular-runtime": "^2.4.0",
    "@types/lodash": "^4.17.20",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.3",
    "@angular/build": "^20.3.3",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.3.0",
    "@angular/platform-browser-dynamic": "^20.3.2",
    "@cypress/schematic": "^4.1.3",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.19.19",
    "cypress": "^15.3.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "netlify-cli": "^23.8.1",
    "netlify-plugin-cypress": "^2.2.1",
    "start-server-and-test": "^2.1.2",
    "typescript": "~5.9.2"
  }
}
